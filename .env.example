# Database
DATABASE_URL="file:./dev.db"

# Authgear (get from https://portal.authgear.com/)
NEXT_PUBLIC_AUTHGEAR_CLIENT_ID=
NEXT_PUBLIC_AUTHGEAR_ENDPOINT=
NEXT_PUBLIC_AUTHGEAR_REDIRECT_URI=http://localhost:3000/auth-redirect

# AI API Configuration
# OpenRouter (recommended): https://openrouter.ai/api/v1
# OpenAI: https://api.openai.com/v1
OPENAI_BASE_URL=https://openrouter.ai/api/v1
OPENAI_API_KEY=

# Model Configuration
# Recommended: bytedance-seed/seed-1.6-flash, google/gemini-2.5-flash
MIDSCENE_MODEL_NAME=google/gemini-2.5-flash
MIDSCENE_MODEL_FAMILY="gemini"
